{
    "version": 2,
    "name": "usms-frontend",
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "buildCommand": "npm install --legacy-peer-deps && npm run build"
  }
  